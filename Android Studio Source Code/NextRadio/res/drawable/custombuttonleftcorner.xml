<?xml version="1.0" encoding="utf-8"?>
<selector>
    <item android:state_pressed="true">
        <shape android:shape="0">
            <corners android:radius="1dp" android:topLeftRadius="20dp" android:topRightRadius="0.1dp" android:bottomLeftRadius="20dp" android:bottomRightRadius="0.1dp" />
            <stroke android:width="1dp" android:color="#ffffff" />
            <gradient android:startColor="#06acd2" android:endColor="#689a92" android:angle="-90.0" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="0">
            <corners android:radius="1dp" android:topLeftRadius="20dp" android:topRightRadius="0.1dp" android:bottomLeftRadius="20dp" android:bottomRightRadius="0.1dp" />
            <stroke android:width="1dp" android:color="#06acd2" />
            <solid android:color="#06acd2" />
        </shape>
    </item>
    <item>
        <shape android:shape="0">
            <corners android:radius="1dp" android:topLeftRadius="20dp" android:topRightRadius="0.1dp" android:bottomLeftRadius="20dp" android:bottomRightRadius="0.1dp" />
            <stroke android:width="1dp" android:color="#06acd2" />
            <gradient android:startColor="#06acd2" android:endColor="#06acd2" android:angle="-90.0" />
        </shape>
    </item>
</selector>
